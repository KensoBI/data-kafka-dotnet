{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kafka": {
    "BootstrapServers": "stream.kensobi.com:9095",
    "SecurityProtocol": "SSL",
    "SslCaLocation": "full\\path\\to\\ca.crt",
    "SslCertificateLocation": "full\\path\\to\\client1_client.pem",
    "SslKeyLocation": "full\\path\\to\\client1_client.key",
    "SslEndpointIdentificationAlgorithm": "none",
    "EnableSslCertificateVerification": false,
    "BasicAuthUserInfo": "client1:password",
    "Topic": "client1-topic",
    "SchemaRegistries": "https://stream.kensobi.com:8099",
    "NumPartitions": 2,
    "ReplicationFactor": 1,
    "GroupId": "client-group"
  }
}
